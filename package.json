{
  "name": "videojs-google-chromecast",
  "homepage": "https://github.com/mikadoplus/videojs-google-chromecast",
  "version": "0.0.3",
  "description": "Chromecast plugin for videojs, adapted for v7.*",
  "main": "dist/videojs-google-chromecast.cjs.js",
  "module": "dist/videojs-google-chromecast.es.js",
  "generator-videojs-plugin": {
    "version": "7.6.3"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all -p build:*",
    "build:css": "node-sass --include-path src --output-style compressed src/css/plugin.scss -o dist",
    "build:js": "npm-run-all build:js:rollup build:js:bannerize build:js:uglify build:js:copy",
    "build:js:bannerize": "bannerize dist/videojs-google-chromecast.js --banner=scripts/banner.ejs",
    "build:js:rollup": "rollup -c scripts/rollup.config.js",
    "build:js:uglify": "uglifyjs dist/videojs-google-chromecast.js --comments --mangle --compress  -o dist/videojs-google-chromecast.min.js",
    "build:js:copy": "cp src/js/sender.js dist/",
    "clean": "rimraf dist test/dist",
    "postclean": "mkdirp dist test/dist",
    "lint": "vjsstandard",
    "start": "npm-run-all -p start:server watch",
    "start:server": "static -a 0.0.0.0 -p 9999 -H '{\"Cache-Control\": \"no-cache, must-revalidate\"}' .",
    "pretest": "npm-run-all lint build",
    "test": "npm run build",
    "preversion": "npm run test",
    "version": "node scripts/version.js",
    "watch": "npm-run-all -p watch:*",
    "watch:css": "nodemon -e scss -x \"npm run build:css\"",
    "watch:js": "rollup -c scripts/rollup.config.js -w",
    "prepare": "npm run build",
    "prepush": "npm run test"
  },
  "keywords": [
    "videojs",
    "videojs-plugin",
    "chromecast"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mikadoplus/videojs-google-chromecast.git"
  },
  "author": "mikadoplus <plo@mikadoplus.lu>",
  "license": "UNLICENSED",
  "vjsstandard": {
    "ignore": [
      "dist",
      "docs",
      "test/dist",
      "examples",
      "scripts/karma.conf.js",
      "src"
    ]
  },
  "files": [
    "CONTRIBUTING.md",
    "dist/",
    "docs/",
    "index.html",
    "scripts/",
    "src/",
    "test/"
  ],
  "dependencies": {
    "global": "^4.4.0",
    "video.js": "^7.7.6"
  },
  "devDependencies": {
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-multi-entry": "^3.0.1",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@videojs/generator-helpers": "^1.1.0",
    "acorn": "^7.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "bannerize": "^1.1.4",
    "conventional-changelog-cli": "^2.0.34",
    "conventional-changelog-videojs": "^3.0.0",
    "core-js": "^3.6.5",
    "fsevents": "^2.1.3",
    "husky": "^4.2.5",
    "karma": "^4.4.1",
    "mkdirp": "^1.0.4",
    "node-sass": "^4.14.1",
    "node-static": "^0.7.11",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "postcss-cli": "^7.1.1",
    "postcss-scss": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "semver": "^7.3.2",
    "sinon": "^9.0.2",
    "videojs-generate-karma-config": "^5.3.1",
    "videojs-generate-rollup-config": "^5.0.2",
    "videojs-standard": "^8.0.4"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  }
}
